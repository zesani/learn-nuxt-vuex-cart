{
  "name": "vue-meta",
  "description": "manage page meta info in Vue 2.0 server-rendered apps",
  "version": "0.5.6",
  "author": "Declan de Wet <declandewet@me.com>",
  "bugs": "https://github.com/declandewet/vue-meta/issues",
  "dependencies": {
    "deepmerge": "^1.2.0",
    "lodash.isplainobject": "^4.0.6",
    "object-assign": "^4.1.0"
  },
  "devDependencies": {
    "babel-cli": "^6.18.0",
    "babel-core": "^6.18.0",
    "babel-loader": "^6.2.7",
    "babel-plugin-istanbul": "^4.1.1",
    "babel-preset-es2015": "^6.18.0",
    "babel-register": "^6.18.0",
    "chai": "^3.5.0",
    "codecov": "^2.1.0",
    "cross-env": "^4.0.0",
    "css-loader": "^0.28.0",
    "doctoc": "^1.2.0",
    "es6-promise": "^4.0.5",
    "express": "^4.14.0",
    "express-urlrewrite": "^1.2.0",
    "file-loader": "^0.11.0",
    "json-loader": "^0.5.4",
    "karma": "^1.3.0",
    "karma-chai": "^0.1.0",
    "karma-coverage": "^1.1.1",
    "karma-mocha": "^1.2.0",
    "karma-mocha-reporter": "^2.2.0",
    "karma-phantomjs-launcher": "^1.0.2",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^2.0.2",
    "mocha": "^3.1.2",
    "phantomjs-prebuilt": "^2.1.13",
    "rimraf": "^2.5.4",
    "rollup": "^0.41.3",
    "rollup-plugin-buble": "^0.15.0",
    "rollup-plugin-commonjs": "^8.0.2",
    "rollup-plugin-json": "^2.0.2",
    "rollup-plugin-node-resolve": "^3.0.0",
    "snazzy": "^6.0.0",
    "standard": "^10.0.0",
    "uglify-js": "^2.7.4",
    "update-section": "^0.3.3",
    "vue": "^2.0.3",
    "vue-loader": "^11.0.0",
    "vue-router": "^2.0.1",
    "vue-template-compiler": "^2.1.0",
    "vuex": "^2.0.0",
    "webpack": "^2.2.1",
    "webpack-dev-server": "^2.4.1"
  },
  "files": [
    "lib",
    "yarn.lock"
  ],
  "homepage": "https://github.com/declandewet/vue-meta",
  "keywords": [
    "attribute",
    "google",
    "head",
    "helmet",
    "info",
    "meta",
    "seo",
    "server",
    "ssr",
    "title",
    "universal",
    "vue"
  ],
  "license": "MIT",
  "main": "lib/vue-meta.js",
  "nyc": {
    "exclude": [
      "test/**/*.js"
    ]
  },
  "repository": {
    "url": "git@github.com:declandewet/vue-meta.git",
    "type": "git"
  },
  "scripts": {
    "build": "rollup -c",
    "codecov": "codecov",
    "deploy": "npm version",
    "dev": "babel-node examples/server.js",
    "lint": "standard --verbose | snazzy",
    "minify": "uglifyjs lib/vue-meta.js -cm --comments -o lib/vue-meta.min.js",
    "postbuild": "npm run minify",
    "postdeploy": "git push origin master --follow-tags && npm run release",
    "postversion": "npm run update-cdn && git add . && git commit -m \":ship: CDN update\"",
    "prebuild": "rimraf lib",
    "predeploy": "git checkout master && git pull -r",
    "prerelease": "npm run build",
    "pretest": "npm run lint",
    "preversion": "npm run toc",
    "release": "npm publish",
    "test": "cross-env NODE_ENV=test karma start karma.conf.js",
    "toc": "doctoc README.md --title '# Table of Contents'",
    "update-cdn": "babel-node scripts/update-cdn.js"
  },
  "standard": {
    "globals": [
      "Vue",
      "define",
      "describe",
      "it",
      "expect",
      "before",
      "beforeEach",
      "after",
      "afterEach"
    ]
  }
}
